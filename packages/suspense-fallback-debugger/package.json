{
  "name": "suspense-fallback-debugger",
  "version": "1.2.3",
  "description": "Suspense Fallback Debugger for React",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "private": false,
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./dropdown-menu": {
      "types": "./dist/components/dropdown-menu.d.mts",
      "default": "./dist/components/dropdown-menu.mjs"
    },
    "./context": {
      "types": "./dist/suspense/context.d.mts",
      "default": "./dist/suspense/context.mjs"
    },
    "./dev": {
      "types": "./dist/dev.d.mts",
      "default": "./dist/dev.mjs"
    },
    "./internal": {
      "types": "./dist/internal/index.d.mts",
      "default": "./dist/internal/index.mjs"
    }
  },
  "files": [
    "dist",
    "src",
    "CHANGELOG.md"
  ],
  "peerDependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "dependencies": {
    "@base-ui-components/react": "1.0.0-beta.6",
    "zustand": "5.0.7"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "6.1.0",
    "@types/node": "22.15.3",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "@workspace/typescript-config": "workspace:*",
    "@workspace/ui": "workspace:*",
    "babel-plugin-react-compiler": "1.0.0",
    "tsdown": "0.16.5",
    "typescript": "5.9.2"
  },
  "keywords": [],
  "author": "Jared-MB",
  "license": "ISC",
  "sideEffects": false
}
